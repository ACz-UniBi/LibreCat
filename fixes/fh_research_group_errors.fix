if all_match('_fif.start_date','.+')

  if all_match('_fif.end_date','.+')

    copy_field('_fif.start_date','_s.$append')
    copy_field('_fif.end_date','_s.$append')

    sort('_s')

    unless in('_fif.start_date','_s.$first')

      add_field('_errors.$append','form_handler.form_errors.start_before_end_date')

    end

    remove_field('_s')

  end

end
