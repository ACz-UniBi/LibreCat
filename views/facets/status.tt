<button data-toggle="collapse" data-target="#status" class="btn-link helpme" data-placement="left"><span class="fa fa-chevron-down"></span> [% h.loc("facets.visibility_status") %]</button>
<div class="facettecollapse">
  <ul id="status" class="collapse">
  [%- FOREACH stat IN facets.status.terms %]
    <li><a class="facet" data-key="cql" data-param="status" data-value="[% stat.term %]" href="#" rel="nofollow">[% stat.term %] ([% stat.count %])</a></li>
  [%- END %]
  </ul>
</div>
