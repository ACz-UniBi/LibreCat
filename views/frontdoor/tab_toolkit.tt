
<!-- BEGIN frontdoor/tab_toolkit.tt -->

<div class="tab-pane" id="toolkit">
[% lf.frontdoor.tabs.toolkit.data_by %]<br><br>
[% FOREACH t IN toolkitData.keys %]
  [% IF t != "_id" %]
  <div class="row">
    <div class="col-md-2 text-muted">[% t %]</div> 
    <div class="col-md-10">
    [% FOREACH d IN toolkitData.$t %]<a href="[% d %]">[% d %]</a><br>[% END %]
    </div>
  </div>
  [% END %]
[% END %]
</div>

<!-- END frontdoor/tab toolkit.tt -->
