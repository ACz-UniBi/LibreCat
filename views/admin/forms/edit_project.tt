<div class="row">

  <div class="col-md-10 col-sm-12 col-md-offset-1"> <!-- main content (left) -->

    <div class="page-header" id="banner">
      <h1>
      [% IF name %]
      Edit Project "[% name %]"
      [% ELSE %]
      Add new project
      [% END %]
      </h1>
    </div>


    <form action="/myPUB/admin/project/update" method="POST">
