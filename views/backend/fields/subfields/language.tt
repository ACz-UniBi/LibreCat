[% language_preselect = h.get_list("language_preselect") %][% language_list = h.get_list("language") %]<div class="form-group row innerrow"><div class="col-md-9"><select name="language.' + index + '.name" class="form-control" id="select_language_' + index + '">[% FOREACH lang IN language_preselect.keys.sort %]<option value="[% lang %]"[% IF lang=="English" %] selected="selected"[% END %]>[% lang %]</option>[% END %]<option value="2436687"></option>[% FOREACH lang IN language_list.keys.sort %]<option value="[% lang %]">[% lang %]</option>[% END %]</select></div><div class="col-md-1"><div class="glyphicon glyphicon-minus single"></div></div></div>